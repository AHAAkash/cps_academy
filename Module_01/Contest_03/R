#include <iostream>
using namespace std;

int main() {
    int tc;
    cin >> tc; 

    while (tc--) {
        int X, Y, Z;
        cin >> X >> Y >> Z;  
        int total_students = X * Y;
        if (Z > total_students / 2) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
