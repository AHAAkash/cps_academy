#include <bits/stdc++.h>
using namespace std;

int main() {
    int N, X;
    cin >> N >> X;
    
    int totalScore = 0;
    for (int i = 0; i < N; i++) {
        int scr;
        cin >> scr;
        if (scr <= X) {
            totalScore += scr;
        }
    }
    cout << totalScore << endl;
    
    return 0;
}
