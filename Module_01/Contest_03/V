#include <bits/stdc++.h>
using namespace std;

int main() {
    int tc;
    cin >> tc;
    while(tc--){
      int a1, a2, b1, b2;  
        cin >> a1 >> a2 >> b1 >> b2;
        
        int export_A = a1 - a2;
        int export_B = b1 - b2;
        
        int export_C = -(export_A + export_B);
        
        if (export_C > 0) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    return 0;
}
