#include <bits/stdc++.h>
using namespace std;

int main() {
     int n;
     cin >> n;
     int arr[7*n];
     for(int i=0;i<7*n;i++){
        cin >> arr[i];
     }
     for(int i=0;i<n;i++){
        int sum=0;
        for(int j=0;j<7;j++){
           sum+=arr[i*7 + j];
        }
        cout << sum << " ";
     }
    return 0;
}
    
